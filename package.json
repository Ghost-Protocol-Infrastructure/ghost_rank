{
  "name": "ghost-rank-indexer",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "index:base": "npx tsx scripts/index-base.ts",
    "index:db": "npx tsx scripts/index-db.ts",
    "score": "npx tsx scripts/score-leads.ts",
    "typecheck": "tsc --noEmit",
    "contracts:compile": "hardhat compile",
    "contracts:deploy:vault:base-mainnet": "node scripts/deploy-vault.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "migrate:db": "npx tsx scripts/migrate-db.ts",
    "migrate:indexer": "npx tsx scripts/migrate-indexer.ts",
    "check:lock": "npx npm@10.8.2 ci --ignore-scripts --no-audit --no-fund",
    "update-data": "npm run index:db",
    "prepare": "husky"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@prisma/client": "^6.16.2",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.20",
    "ethers": "^6.15.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "prisma": "^6.16.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "^2.21.0",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-verify": "^3.0.10",
    "@nomicfoundation/hardhat-viem": "^3.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.11.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "hardhat": "^3.1.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20 <21",
    "npm": "^10.8.2"
  },
  "lint-staged": {
    "package.json": [
      "node -e \"require('child_process').execSync('npx npm@10.8.2 install --package-lock-only --ignore-scripts', { stdio: 'inherit' })\"",
      "git add package-lock.json"
    ]
  }
}
