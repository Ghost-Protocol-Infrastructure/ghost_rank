{
  "name": "ghost-rank-indexer",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "indexer": "tsx indexer.ts",
    "indexer:once": "tsx indexer.ts --once",
    "typecheck": "tsc --noEmit",
    "contracts:compile": "hardhat compile",
    "contracts:deploy:base-sepolia": "hardhat run scripts/deploy.ts --network baseSepolia",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check:lock": "npx npm@10.8.2 ci --ignore-scripts --no-audit --no-fund",
    "update-data": "npx tsx indexer.ts --once"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.20",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "^2.21.0",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-verify": "^3.0.10",
    "@nomicfoundation/hardhat-viem": "^3.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.11.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "hardhat": "^3.1.7",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20 <21",
    "npm": "^10.8.2"
  }
}
